# Production Environment Variables for FreelanceHub Client

# API Configuration - Update with your production API URL
VITE_API_URL=/api

# Socket.io Configuration - Update with your production domain
VITE_SOCKET_URL=

# App Configuration
VITE_APP_NAME=FreelanceHub
VITE_APP_VERSION=1.0.0

# Stripe Configuration (Production Public Key)
VITE_STRIPE_PUBLISHABLE_KEY=pk_live_your_production_stripe_publishable_key

# Cloudinary Configuration (for client-side uploads)
VITE_CLOUDINARY_CLOUD_NAME=your_production_cloudinary_cloud_name
VITE_CLOUDINARY_UPLOAD_PRESET=your_production_upload_preset

# Feature Flags
VITE_ENABLE_VOICE_MESSAGES=true
VITE_ENABLE_PAYMENT_REQUESTS=true
VITE_ENABLE_FILE_SHARING=true

# Production Configuration
VITE_DEV_MODE=false
VITE_DEBUG_MODE=false
